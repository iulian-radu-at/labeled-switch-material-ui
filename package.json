{
  "name": "labeled-switch-material-ui",
  "version": "3.3.0",
  "description": "A material-ui component which implements a iOS like switch with a label on each side",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production yarn build && yarn publish --non-interactive",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -c storybook",
    "build-storybook": "build-storybook -c storybook -o docs",
    "commit-storybook": "git add . && git commit -m \"Updated storybook\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/labeled-switch-material-ui.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "text",
    "labeled-switch",
    "password",
    "material-ui",
    "labeled-switch-material-ui",
    "react",
    "react-component",
    "ui",
    "material design"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/labeled-switch-material-ui/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/labeled-switch-material-ui#readme",
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "ios-switch-material-ui": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/react": "^5.3.17",
    "@types/babel-core": "^6.25.6",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/rimraf": "^3.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules",
      "./history"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ]
  }
}
